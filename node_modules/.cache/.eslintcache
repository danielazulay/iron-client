e}\n        onChange={handleChange}\n        name=\"price\"\n        required\n      />\n      <div className=\"form-group\">\n      <form action=\"/action_page.php\">\n    <label htmlFor=\"img\">Select image:</label>\n    <input type=\"file\" id=\"img\" name=\"img\" accept=\"image/*\"></input>\n    </form>\n        <button className=\"btn btn-primary\" type=\"submit\" >\n          Salvar\n        </button>\n      </div>\n      </form>\n  );\n}\n\nexport default EditProduct;","/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/auth/Profile.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/auth/Signup.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/routerComponents/account/EditeUser.js",[],"/mnt/c/ironhack/e-comerce/iron-client/src/Products/ProductDetail.js",["215"],"import React, { useEffect, useState, useContext } from \"react\";\nimport api from \"../apis/api\";\nimport { useParams, Link } from \"react-router-dom\";\nimport CardProducts from \"../components/CardProducts\";\nimport { CartContext } from \"../contexts/cartContext\";\nimport { AuthContext } from \"../contexts/authContext\";\n\nfunction ProductDetails() {\n  const [state, setState] = useState({\n    userid: [],\n    name: \"\",\n    description: \"\",\n    size: \"\",\n    price: \"\",\n    id:\"\",\n    img: \"\"\n  });\n  const { loggedInUser, logoff } = useContext(AuthContext);\n  const [quantity, setQuantity] = useState(0);\n  const { id } = useParams();\n  const { cart, setCart } = useContext(CartContext);\n\n  useEffect(() => {\n    async function fetchDetails() {\n      try {\n        const response = await api.get(`/productDetails/${id}`);\n        console.log(\"eu sou o productDetails --> \", response);\n        setState({ ...response.data });\n      } catch (err) {\n        console.log(err);\n      }\n    }\n    fetchDetails();\n  }, [id]);\nconsole.log(cart)\n  return (\n    <div>\n      <CardProducts\n      id={state._id}\n      image={state.img}\n        name={st